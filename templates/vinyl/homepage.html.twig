{% extends 'base.html.twig' %}

{% block title %} Create new Record | {{ parent() }} {% endblock %}

{% block body %}

<h1>{{ title }}</h1>

<div>
    Some tracks:
    {{ dump() }}
    <ul>
        {% for track in tracks %}

            <li>
                {{ track.artist }} - {{ track.song }}
            </li>

        {% endfor %}

    </ul>
</div>



{% endblock %}